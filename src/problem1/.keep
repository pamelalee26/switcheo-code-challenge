var sum_to_n_a = function(n) {
    // for loop
    let sum = 0;
    for (let i = 1; i <= n; i++){
        sum += i;
    }
    return sum;
};

var sum_to_n_b = function(n) {
    // even number
    if (n % 2 == 0){
        return (n+1) * (n/2);
    }
    // odd number
    else{
        return Math.floor(n / 2) * (n + 1) + (n / 2 + 1);
    }
};

var sum_to_n_c = function(n) {
    // arithmetic series 
    let sum = n/2 * (2 + (n-1));
    return sum;
};